                        "error": [
                          {
                            "detail": "Unable to update Materiai",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BibliotecaDB.updateMateriai",
                        "parameters": [
                          {
                            "name": "MaterialID",
                            "value": "${parameters.MaterialID}"
                          },
                          {
                            "name": "Materiai",
                            "value": "${materiai}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${materiai}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "A2019128143.Models.BibliotecaDb.Materiai",
                  "data": "${materiai}",
                  "visible": "${materiai != null}"
                }
              ]
            }
