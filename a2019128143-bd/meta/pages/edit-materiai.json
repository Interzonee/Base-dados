  "events": {
    "load": [
      {
        "name": "BibliotecaDB.getMateriaiByMaterialID",
        "parameters": [
          {
            "name": "MaterialID",
            "value": "${parameters.MaterialID}"
          }
        ],
        "then": [
          {
            "name": "materiai",
            "type": "set",
            "value": "${result}",
            "valueType": "A2019128143.Models.BibliotecaDb.Materiai"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
